package bankTransactions;

public class Account {
	private int actid;
	private String name;
	private Double bal;
	
	public Account(int actid, String name, Double bal) {
		super();
		this.actid = actid;
		this.name = name;
		this.bal = bal;
	}
	
	public withdraw(Double amt) {
		bal -= amt;
	}
	
	
	
	
}
