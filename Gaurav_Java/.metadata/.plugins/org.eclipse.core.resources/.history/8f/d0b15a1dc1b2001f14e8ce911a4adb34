package dateFormat;

public class MyMath {
	public static int add(int i, int j) {
		return i+j;
	}
	public static int multi(int i, int j) {
		return i*j;
	}
	public static int subs(int i, int j) {
		return i-j;
	}
	public static int div(int i, int j) {
		if(j==0) throws new ArithmeticException("Plz Enter num2 >0");
		else
		return i/j;
	}
}
