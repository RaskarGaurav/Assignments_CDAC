package dateFormat;

import java.text.SimpleDateFormat;

public class Product {
	private int prdid;
	private String name;
	private Double price;
	private Category prdCat;
	
	private static SimpleDateFormat sdf;

	public Product(int prdid, String name, Category prdCat) {
		super();
		this.prdid = prdid;
		this.name = name;
		this.prdCat = prdCat;
	}
}	
