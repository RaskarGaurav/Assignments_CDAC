package innerClass;

public class Account {
	private int actid;//auto increment for actid
	private String name;
	private String actType;
	
	public Account() {
		actid =101;
		name="Walter_White";
		actType = "No type yet";
	}
	
	public Account(int actid, String name, String actType) {
		this.actid = actid;
		this.name = name;
		this.actType = actType;
	}

	public void display() {
		System.out.println("Account No.: "+actid+"\nName: "+name+"\nAccount Type: "+actType);
	}
}
